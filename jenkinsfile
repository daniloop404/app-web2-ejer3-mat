node {
    stage('Revision'){
        checkout scm
    }

    stage('Instalaci√≥n') {
        bat 'npm install'
    }

    stage('Construir'){
        bat 'npx ng build'
    }

    stage('Limpiar destino'){
        bat 'rmdir /s /q C:\\servidor\\fire'
    }

    stage('Mover al servidor'){
        bat 'xcopy C:\\ProgramData\\Jenkins\\.jenkins\\workspace\\angular-pipeline\\dist\\app-03\\browser C:\\servidor\\fire /E /I'
    }
}